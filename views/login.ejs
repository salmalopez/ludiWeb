<!DOCTYPE html>
<html ng-app="adminApp">
  <head>
    <title>Administrador</title>

    <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href='https://fonts.googleapis.com/css?family=Indie+Flower' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Shadows+Into+Light|Amatic+SC' rel='stylesheet' type='text/css'>
    <!--STYLES-OUT-->
    <link rel="stylesheet" href="/OUT/styles/animate.css">
    <link rel="stylesheet" href="/OUT/styles/bootstrap.min.css">
    <link rel="stylesheet" href="/OUT/styles/importer.css">
    <link rel="stylesheet" href="/OUT/styles/normalize.css">
    <!--STYLES-OUT END-->
  </head>

  <body>
    
    <div class="container">
        <div class="row">
            <div class="col-sm-6 col-md-4 col-md-offset-4">
                <h1 class="text-center login-title">LOGIN</h1>
                <div class="account-wall">
                    <img class="profile-img" src="https://lh5.googleusercontent.com/-b0-k99FZlyE/AAAAAAAAAAI/AAAAAAAAAAA/eu7opA4byxI/photo.jpg?sz=120"
                        alt="">
                    <form class="form-signin">
                    <input type="text" ng-model="profile.email" class="form-control" placeholder="Correo electrónico">
                    <input type="password" ng-model="profile.password" class="form-control" placeholder="Contraseña">
                    <button ng-click="login()" class="btn btn-lg btn-primary btn-block" type="submit">
                        Ingresar</button>
                    
                    </form>
                </div>
            </div>
        </div>
    </div>

    <!--SCRIPTS-OUT -->
    <!--SCRIPTS-OUT END-->
  </body>
</html>
